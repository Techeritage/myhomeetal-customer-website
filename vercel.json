{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://my-home-et-al-backend-u0m7.onrender.com/api/v1/$1",
        "headers": {
          "cache-control": "s-maxage=60, stale-while-revalidate"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "functions": {
      "api/*.js": {
        "maxDuration": 30
      }
    }
  }